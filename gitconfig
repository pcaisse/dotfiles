[core]
  excludesfile = ~/.gitignore
[push]
  default = simple
[alias]
  l = log --graph --oneline
  lp = log -p
  lp1 = log -p -n 1
  lf = log -u
  ln = log --name-only
  cp = cherry-pick
  st = status
  cl = clone
  ci = commit -v
  co = checkout
  br = branch
  d = diff
  dc = diff --cached
  amend = commit --amend --no-edit
